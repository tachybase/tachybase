{
  "name": "tachybase",
  "version": "1.3.15",
  "private": true,
  "license": "Apache-2.0",
  "scripts": {
    "build": "tachybase build",
    "build:p": "tachybase build -s --no-dts",
    "clean": "tachybase clean",
    "dev": "tachybase dev",
    "dev-local": "APP_ENV_PATH=.env.local tachybase dev",
    "dev-server": "tachybase dev --server",
    "e2e": "tachybase e2e",
    "preinstall": "npx only-allow pnpm",
    "postinstall": "tachybase postinstall",
    "lint": "oxlint",
    "pm": "tachybase pm",
    "pm2": "tachybase pm2",
    "prepare": "husky",
    "start": "tachybase start",
    "tachybase": "tachybase",
    "tb": "tachybase",
    "tbi": "tachybase install",
    "tbu": "tachybase upgrade",
    "tc": "tachybase test:client",
    "tego": "tego",
    "test": "tachybase test",
    "test:client": "tachybase test:client",
    "test:server": "tachybase test:server",
    "ts": "tachybase test:server"
  },
  "lint-staged": {
    "*.{js,json,sql,ts,tsx}": [
      "prettier --write"
    ],
    "*.ts?(x)": [
      "oxlint --fix"
    ]
  },
  "dependencies": {
    "pm2": "6.0.8",
    "rimraf": "6.0.1"
  },
  "devDependencies": {
    "@commitlint/cli": "19.8.1",
    "@commitlint/config-conventional": "19.8.1",
    "@commitlint/prompt-cli": "19.8.1",
    "@ianvs/prettier-plugin-sort-imports": "4.4.2",
    "@inquirer/prompts": "7.5.3",
    "@tachybase/test": "workspace:*",
    "@tego/devkit": "workspace:*",
    "@types/node": "20.17.10",
    "@types/react": "^18.3.18",
    "@types/react-dom": "^18.3.5",
    "husky": "9.1.7",
    "lint-staged": "16.1.2",
    "oxlint": "1.3.0",
    "prettier": "3.6.0",
    "prettier-plugin-packagejson": "2.5.17",
    "prettier-plugin-sort-json": "4.1.1",
    "prettier-plugin-sql": "0.19.1",
    "pretty-format": "30.0.2",
    "pretty-quick": "4.2.2",
    "react": "18.3.1",
    "react-dom": "18.3.1",
    "tego": "workspace:*",
    "tsx": "4.20.3",
    "vitest": "3.2.4"
  },
  "packageManager": "pnpm@10.12.4",
  "engines": {
    "node": ">=20.18.0"
  },
  "pnpm": {
    "neverBuiltDependencies": [
      "canvas"
    ],
    "overrides": {
      "@types/node": "20.17.10",
      "antd": "5.22.5",
      "dayjs": "1.11.13",
      "react-router": "6.28.1",
      "react-router-dom": "6.28.1",
      "string-width": "4.2.3",
      "strip-ansi": "6.0.1",
      "wrap-ansi": "7.0.0"
    }
  }
}
